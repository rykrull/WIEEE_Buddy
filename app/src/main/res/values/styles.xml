<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <style name="WIEEE" parent="Theme.AppCompat.NoActionBar" >
        <item name="android:colorBackground">@color/background_holo_dark</item>
        <item name="android:windowBackground">@drawable/screen_background_selector_dark</item>
    </style>
    <style name="WIEEE.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />
    <style name="WIEEE.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />

    <style name="Theme.WIEEE2" parent="Theme.AppCompat" />

    <style name="WIEEE2_NoActionBar" parent="Theme.AppCompat.NoActionBar" />

</resources>
